{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "go/*": [".typego/types/*"]
    },
    "skipLibCheck": true
  },
  "include": ["src/**/*", ".typego/types/**/*"]
}